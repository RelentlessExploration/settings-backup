{
  "[markdown]": {
    "editor.quickSuggestions": true
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "files.associations": {
    "*.xml": "html",
    "*.svg": "html"
  },
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.maxTokenizationLineLength": 2000000,
  "editor.fontSize": 18,
  "terminal.integrated.fontSize": 18,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.iconTheme": "material-icon-theme",
  "C_Cpp.default.cppStandard": "c++11",
  "C_Cpp.default.intelliSenseMode": "macos-clang-x64",
  "workbench.startupEditor": "none",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.autoSave": "onFocusChange",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },

  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "[python]": {
    "editor.defaultFormatter": null,
    "editor.tabSize": 4,
    "editor.formatOnPaste": false
  },
  "python.formatting.blackArgs": ["--line-length", "120"],
  "python.formatting.provider": "black",
  "editor.formatOnPaste": true,
  "vim.easymotion": true,
  "vim.statusBarColorControl": false,
  "explorer.confirmDragAndDrop": false,
  "cSpell.userWords": ["asynciterable", "curr", "imshow", "Wechat"],
  "[cpp]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "editor.tabSize": 2,
  "prettier.tabWidth": 2,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/node_modules": true
  },
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "cSpell.language": "en,de",
  "C_Cpp.updateChannel": "Insiders",
  "editor.inlineSuggest.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "material-icon-theme.files.associations": {
    "DEVLOG.md": "changelog",
    "PLAN.md": "contributing"
  },
  "material-icon-theme.folders.associations": {
    "ISSUE_TEMPLATE": "template",
    "test-utils": "test",
    "gateway": "middleware",
    "shibe": "database",
    "entity": "rules",
    "resolvers": "controller",
    "sub-graphql": "graphql",
    "oauth": "secure",
    "test-data": "test"
  },
  "typescript.tsserver.log": "off",
  "files.trimTrailingWhitespace": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "cSpell.enableFiletypes": ["svelte"],
  "python.languageServer": "Jedi",
  "editor.semanticTokenColorCustomizations": {
    "[Default Dark+]": {
      "rules": {
        "variable.defaultLibrary": "#4EC9B0",
        "variable.readonly": "#51a3c4"
      }
    }
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "source.python",
        "settings": {
          "foreground": "#9CDCFE"
        }
      },
      {
        "scope": "punctuation.separator",
        "settings": {
          "foreground": "#D4D4D4"
        }
      }
    ]
  }
}
